<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h2 class="welcome">Welcome <span class="member-name"></span></h2>
    </div>
  </div>

  <div class="row">
    <div class="col-md-4">
      <div class="card">
        <div class="card-body">
          <section class="future-row m-4 ">
            <h2> 5-Day Forecast:</h2>
            <h5 class="cityLabel"></h5>
            <div class="future"></div>
          </section>
        </div>
      </div>
    </div>
    <div class="col-md-8">
      <div class="card">
        <div class="card-body">
          <h2 class="desLabel">Choose your destination!</h2>
          <h3 class="cityLabel"></h3>
          <div class="row">
            <div class="col-md-12 dropnbtn">
              <div class="dropdown multi-level">
                <div class="dropdown d-inline-block">
                  <button class="btn btn-secondary dropdown-toggle dropdownBtn" type="button" id="dropdownMenuButton"
                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    States and Cities
                  </button>
                  <ul class="dropdown-menu multi-level" role="menu" aria-labelledby="dropdownMenu">
                    <li class="dropdown-divider"></li>
                    {{!-- handlebars dropdown --}}
                    {{#each states}}
                    <li class="dropdown-submenu">
                      <a class="dropdown-item stateDown" value="{{state}}" tabindex="-1" href="#">{{ state }}</a>
                      <ul class="dropdown-menu">
                        {{#each cities}}
                        <li class="dropdown-item cityDown"><a href="#">{{ this }}</a></li>
                        {{/each}}
                      </ul>
                    </li>
                    {{/each}}
                  </ul>
                </div>
              </div>
              <button type="submit" class="btn btn-success " id="city-btn">
                <span class="fa fa-thumbs-up"></span>
              </button>
            </div>
          </div>

          {{!-- calendar --}}
          <div class="row calendars">
            <div class="form-group col-md-6">
              <form action="/action_page.php" class="start">
                <label for="travelStartDate">Start Date:</label>
                <input type="date" id="travelStartDate" name="travelStartDate">
                <input type="submit" value="Submit">
              </form>
            </div>
            <div class="form-group col-md-6">
              <form action="/action_page.php" class="end">
                <label for="travelEndDate">End Date:</label>
                <input type="date" id="travelEndDate" name="travelEndDate">
                <input type="submit" value="Submit">
              </form>
            </div>
          </div>

        </div>
      </div>
    </div>
