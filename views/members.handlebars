<link href="stylesheets/members.css" rel="stylesheet">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/logout">Log Out</a>
    </div>
  </div>
</nav>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h3>Welcome <span class="member-name"></span></h3>
    </div>
  </div>
  <div class="jumbotron">
    <div class="row">
      <div class="col-md-4">
        <h1>Travel N Style</h1>
        <br>
        <h4>Enter a city</h4>
        <panel>
          <form class="create-form">
            <div class="form-group">
              <input id="city" placeholder="enter city"></input>
              <button type="submit" class="button" id="city-btn">
                <span class="fa fa-search"></span>
              </button>
            </div>
          </form>
        </panel>
      </div>
      <div class="col-md-8">
        <section>
          <div class="card m-2">
            <h2 class="currentcity m-4"></h2>
            <p class="current ml-4" id="ctemp">
            <p class="current ml-4" id="chumid"></p>
            <p class="current ml-4" id="cwind"></p>
            <p class="current ml-4" id="cuv"><span class="cSpan"></span></p>
          </div class="card">
        </section>
      </div>
    </div>
  </div>
  <div id="outfit">
    <div class="row">
      {{!-- this is for top --}}
      <div class="card">
        <div id="top" class="carouselExampleIndicators carousel slide" data-interval="false">
          <ol class="carousel-indicators">
            <li data-target="#top" data-slide-to="0" class="active"></li>
            <li data-target="#top" data-slide-to="1"></li>
            <li data-target="#top" data-slide-to="2"></li>
          </ol>
          <div class="carousel-inner" id="top1Img"></div>
          <a class="carousel-control-prev" href="#top" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#top" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
      {{!-- this is for bottomware --}}
      <div id="bottom" class="carouselExampleIndicators carousel slide card" data-interval="false" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#bottom" data-slide-to="0" class="active"></li>
          <li data-target="#bottom" data-slide-to="1"></li>
          <li data-target="#bottom" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" id="bottom1Img"></div>
        <a class="carousel-control-prev" href="#bottom" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#bottom" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      {{!-- this is for shoes --}}
      <div id="shoe" class="carouselExampleIndicators carousel slide card" data-interval="false" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#shoe" data-slide-to="0" class="active"></li>
          <li data-target="#shoe" data-slide-to="1"></li>
          <li data-target="#shoe" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" id="shoe1Img"></div>
        <a class="carousel-control-prev" href="#shoe" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#shoe" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      {{!-- this is for accessories --}}
      <div id="accessory" data-interval="false" class="carouselExampleIndicators  carousel mr-auto slide card"
        data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#accessory" data-slide-to="0" class="active"></li>
          <li data-target="#accessory" data-slide-to="1"></li>
          <li data-target="#accessory" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" id="accessory1Img"></div>
        <a class="carousel-control-prev" href="#accessory" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#accessory" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      <button id="fav-fit">your fav</button>
      <button id="saved-fav">saved fav</button>
    </div>
  </div>

  {{!-- new carosaul for saved outfit --}}
  <div id="showFav" data-interval="false" class="carouselExampleIndicators  carousel mr-auto slide card"
    data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#showfav" data-slide-to="0" class="active"></li>
      <li data-target="#showfav" data-slide-to="1"></li>
      <li data-target="#showfav" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" id="showfav1"></div>
    <a class="carousel-control-prev" href="#showFav" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#showFav" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>

